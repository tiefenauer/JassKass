			   creationComplete="application1_creationCompleteHandler(event)"
			   >
	<fx:Script>
		<![CDATA[
			import mx.events.FlexEvent;
			
			import org.flexunit.listeners.AirCIListener;
			import org.flexunit.runner.FlexUnitCore;
			
			//UNIT_TESTS_IMPORT_START
	                import test.info.tiefenauer.SampleTest;
	                import test.info.tiefenauer.gdas3.SpreadsheetAuthorTest;
	                import test.info.tiefenauer.gdas3.SpreadsheetCategoryTest;
	                import test.info.tiefenauer.gdas3.SpreadsheetContentTest;
	                import test.info.tiefenauer.gdas3.SpreadsheetLinkTest;
	                import test.info.tiefenauer.gdas3.SpreadsheetServiceTest;
	                import test.info.tiefenauer.gdas3.SpreadsheetTest;
	                import test.info.tiefenauer.jasskass.app.controllers.GetOAuth2AccessTokenTest;
	                import test.info.tiefenauer.jasskass.app.controllers.LoadAppConfigurationTest;
	                import test.info.tiefenauer.jasskass.app.controllers.SaveOAuth2AccessTokenTest;
	                import test.info.tiefenauer.jasskass.app.models.ConfigProxyTest;
	                import test.info.tiefenauer.jasskass.app.models.FileServiceTest;
	                import test.info.tiefenauer.jasskass.app.models.SettingProxyTest;
